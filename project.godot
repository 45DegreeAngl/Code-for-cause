; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Love Drunk"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/image="res://Assets/Textures/Disclaimer.png"
config/icon="res://icon Image.png"

[autoload]

Actions="*res://Globals/Actions.gd"
Globals="*res://Globals/Globals.gd"
MainShaderCanvas="*res://Shaders/Main Shader Canvas.tscn"
GlobalSteam="*res://Globals/GlobalSteam.gd"

[display]

window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Code-for-cause"

[file_customization]

folder_colors={
"res://Assets/": "orange",
"res://DEBUG/": "purple",
"res://Deprecated/": "red",
"res://Globals/": "gray",
"res://Material/": "green",
"res://Resources/": "pink",
"res://Shaders/": "blue"
}

[filesystem]

import/blender/enabled=false

[global_group]

"Please Move"="Used for collision hitbox handling of debris "

[physics]

common/physics_interpolation=true
