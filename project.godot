config_version=5

[application]

config/name="Love Drunk"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/image="res://Assets/Textures/Disclaimer.png"
config/icon="res://icon Image.png"

[autoload]

Actions="*res://Globals/Actions.gd"
Globals="*res://Globals/Globals.gd"
MainShaderCanvas="*res://Shaders/Main Shader Canvas.tscn"
GlobalSteam="*res://Globals/GlobalSteam.gd"
GlobalWorkshop="*res://Globals/GlobalWorkshop.gd"

[display]

window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Code-for-cause"

[editor]

export/convert_text_resources_to_binary=false

[file_customization]

folder_colors={
"res://Assets/": "orange",
"res://DEBUG/": "purple",
"res://Deprecated/": "red",
"res://Globals/": "gray",
"res://Localization stuff/": "teal",
"res://Material/": "green",
"res://Resources/": "pink",
"res://Shaders/": "blue"
}

[filesystem]

import/blender/enabled=false

[global_group]

"Please Move"="Used for collision hitbox handling of debris "

